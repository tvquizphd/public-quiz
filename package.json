{
  "name": "public-quiz-device",
  "version": "3.0.5",
  "description": "Access TV Quiz PhD",
  "scripts": {
    "tsc": "tsc",
    "action": "./action/index",
    "build": "tsc && pnpm pkg",
    "lint": "eslint src docs",
    "lint:src": "eslint src",
    "lint:docs": "eslint docs",
    "develop": "ts-node src/index.ts",
    "pkg": "pkg -t node18-linux-x64 --no-bytecode --public-packages \"*\" --public --out-dir ./action dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvquizphd/public-quiz-device.git"
  },
  "author": "tvquizphd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tvquizphd/public-quiz-device/issues"
  },
  "homepage": "https://github.com/tvquizphd/public-quiz-device#readme",
  "dependencies": {
    "@nthparty/opaque": "^0.3.0",
    "@octokit/graphql": "^5.0.1",
    "argon2": "^0.29.1",
    "eccrypto": "^1.1.6",
    "libsodium-wrappers": "0.7.8",
    "octokit": "^2.0.7",
    "project-sock": "1.0.0-beta.4.0",
    "simple-git": "^3.14.1",
    "ts-node": "^10.9.1"
  },
  "pnpm": {
    "overrides": {
      "node-abi": "3.25.0",
      "libsodium-wrappers-sumo": "0.7.8"
    }
  },
  "devDependencies": {
    "@types/eccrypto": "^1.1.3",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/node": "^18.7.19",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "pkg": "^5.8.0",
    "typescript": "^4.8.3"
  }
}
